  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.12.0",
    "@tatumio/tatum": "^4.2.0",
    "@types/express-rate-limit": "^5.1.3",
    "@types/node-cache": "^4.1.3",
    "@types/uuid": "^10.0.0",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^8.0.1",
    "fs-extra": "^11.1.1",
    "helmet": "^7.2.0",
    "joi": "^17.11.0",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.3",
    "pg": "^8.16.3",
    "prisma": "^6.12.0",
